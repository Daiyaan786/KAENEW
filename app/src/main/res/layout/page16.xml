<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kae_background1">

    <Button
        android:id="@+id/button9"
        android:layout_width="109dp"
        android:layout_height="67dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/rounded_input_background"
        android:backgroundTint="@color/redd"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="377dp"
        android:layout_height="35dp"
        android:layout_marginStart="18dp"
        android:text="Tick one box if student is submiting a video or atending face to face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText76" />

    <CheckBox
        android:id="@+id/checkBox32"
        android:layout_width="108dp"
        android:layout_height="62dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:text="Video"
        app:layout_constraintStart_toEndOf="@+id/checkBox31"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <CheckBox
        android:id="@+id/checkBox31"
        android:layout_width="149dp"
        android:layout_height="65dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="13dp"
        android:text="Face-To-Face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="32dp"
        android:text="KOORSANG/CHOIR 2023 "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText73"
        android:layout_width="228dp"
        android:layout_height="76dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/rounded_input_background"
        android:ems="10"
        android:inputType="text"
        android:text="VANNE EN NAME"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView33" />

    <EditText
        android:id="@+id/editTextText74"
        android:layout_width="228dp"
        android:layout_height="76dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/rounded_input_background"
        android:ems="10"
        android:inputType="text"
        android:text="NAAM VAN SKOOL	 	 "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText73" />

    <EditText
        android:id="@+id/editTextText75"
        android:layout_width="228dp"
        android:layout_height="76dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/rounded_input_background"
        android:ems="10"
        android:inputType="text"
        android:text="GETAL KOORLEDE		 		 "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText74" />

    <EditText
        android:id="@+id/editTextText76"
        android:layout_width="228dp"
        android:layout_height="76dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_input_background"
        android:ems="10"
        android:inputType="text"
        android:text="SK "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText75" />
</androidx.constraintlayout.widget.ConstraintLayout>