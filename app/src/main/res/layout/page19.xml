<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kae_background1">

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="340dp"
        android:background="@drawable/rounded_input_background"
        android:backgroundTint="@color/redd"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="32dp"
        android:text="INSKRYWINGSVORM: KUNS	 "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText79"
        android:layout_width="229dp"
        android:layout_height="54dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="43dp"
        android:ems="10"
        android:inputType="text"
        android:text="VANNE EN NAME"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView39" />

    <EditText
        android:id="@+id/editTextText80"
        android:layout_width="227dp"
        android:layout_height="58dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="text"
        android:text="Klas "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText79" />

    <EditText
        android:id="@+id/editTextText81"
        android:layout_width="223dp"
        android:layout_height="62dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:inputType="text"
        android:text="Skool Graad "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText80" />

    <EditText
        android:id="@+id/editTextText82"
        android:layout_width="225dp"
        android:layout_height="55dp"
        android:layout_marginStart="41dp"
        android:ems="10"
        android:inputType="text"
        android:text="Onderwerp van prent	 "
        app:layout_constraintStart_toStartOf="parent"
