<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kae_background1">

    <Button
        android:id="@+id/button32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="14dp"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView70" />

    <EditText
        android:id="@+id/editTextText181"
        android:layout_width="375dp"
        android:layout_height="67dp"
        android:layout_marginStart="17dp"
        android:ems="10"
        android:inputType="text"
        android:text="Duet/Trio/Ensemble JP (hoveelheid deelnemers) "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText187" />

    <CheckBox
        android:id="@+id/checkBox63"
        android:layout_width="94dp"
        android:layout_height="54dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="7dp"
        android:text="Video"
        app:layout_constraintStart_toEndOf="@+id/checkBox64"
        app:layout_constraintTop_toBottomOf="@+id/textView70" />

    <EditText
        android:id="@+id/editTextText182"
        android:layout_width="213dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="text"
        android:text="Skool graad "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText186" />

    <EditText
        android:id="@+id/editTextText185"
        android:layout_width="211dp"
        android:layout_height="64dp"
        android:layout_marginStart="16dp"
        android:ems="10"
        android:inputType="text"
        android:text="Dromstel JP "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText182" />

    <EditText
        android:id="@+id/editTextText186"
        android:layout_width="210dp"
        android:layout_height="65dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="62dp"
        android:ems="10"
        android:inputType="text"
        android:text="VANNE EN NAME"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView69" />

    <CheckBox
        android:id="@+id/checkBox64"
        android:layout_width="133dp"
        android:layout_height="51dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="15dp"
        android:text="Face-To-Face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView70" />

    <TextView
        android:id="@+id/textView70"
        android:layout_width="377dp"
        android:layout_height="35dp"
        android:layout_marginStart="17dp"
        android:text="Tick one box if student is submiting a video or atending face to face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText181" />

    <EditText
        android:id="@+id/editTextText187"
        android:layout_width="209dp"
        android:layout_height="65dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="7dp"
        android:ems="10"
        android:inputType="text"
        android:text="Perkussie JP "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText185" />

    <TextView
        android:id="@+id/textView69"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="32dp"
        android:text="Junior Dromstel/Percussion	 "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>