<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kae_background1">

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/rounded_input_background"
        android:backgroundTint="@color/redd"
        android:text="Submit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="32dp"
        android:text="Meerstemmige Sang : Senior 	 "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/checkBox41"
        android:layout_width="81dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="22dp"
        android:text="Video"
        app:layout_constraintStart_toEndOf="@+id/checkBox42"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />

    <EditText
        android:id="@+id/editTextText111"
        android:layout_width="223dp"
        android:layout_height="62dp"
        android:layout_marginStart="17dp"
        android:ems="10"
        android:inputType="text"
        android:text="SS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText114" />

    <EditText
        android:id="@+id/editTextText114"
        android:layout_width="221dp"
        android:layout_height="61dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:inputType="text"
        android:text="GETAL SANGERS		 		 "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText116" />

    <EditText
        android:id="@+id/editTextText115"
        android:layout_width="227dp"
        android:layout_height="56dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="31dp"
        android:ems="10"
        android:inputType="text"
        android:text="VANNE EN NAME"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView47" />

    <EditText
        android:id="@+id/editTextText116"
        android:layout_width="223dp"
        android:layout_height="64dp"
        android:layout_marginStart="20dp"
        android:ems="10"
        android:inputType="text"
        android:text="Skool"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText115" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="377dp"
        android:layout_height="35dp"
        android:layout_marginStart="17dp"
        android:text="Tick one box if student is submiting a video or atending face to face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText111" />

    <CheckBox
        android:id="@+id/checkBox42"
        android:layout_width="133dp"
        android:layout_height="51dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="21dp"
        android:text="Face-To-Face"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView48" />
</androidx.constraintlayout.widget.ConstraintLayout>